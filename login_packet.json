keyPair = KeyPairGenerator.getInstance("RSA").genKeyPair()

payload = {
    "identityPublicKey": new String(keyPair.getPublic().getEncoded()),
    "extraData": {
        "displayName": accountName,
        "identity": UUID.randomUUID().toString(),
        "XUID": null
    }
}

certData = {
    "chain": [
        new JWSObject(new JWSHeader(JWSAlgorithm.HS_256), new Payload(payload)).sign(new MACSigner("fakeKey")).serialize()
    ]
}

clientData = {
    "GameVersion": "1.6.1",
    "IsEduMode": false,
    "LanguageCode": "en_US",
    "ServerAddress": serverAddress
}

skinData = new JWSObject(new JWSHeader(JWSAlgorithm.RS256), new Payload(clientData)).sign(new RSASSASigner((RSAPrivateKey) keyPair.getPrivate())).serialize()
